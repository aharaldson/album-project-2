<%- include('../partials/header') %>

<table id="list">
    <thead>
      <tr>
        <th>Artist</th>
        <th>Title</th>
        <th>Release<br>Year</th>
      </tr>
    </thead>
    <tbody>
      <% albums.forEach(function(album) { %>
        <tr>
            <td><%= album.artist %></td>
            <td><%= album.title %></td>
            <td><%= album.releaseYear %></td>
            <td><a href="/albums/<%= album._id %>">DETAILS</a></td>
        </tr>
        <% }); %>
      </tbody>
    </table>

<%- include('../partials/footer') %>